/* pages/choiceVictory/choiceVictory.wxss */
page{
  height: 100%;
}
.content{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.top{
  flex: 1;
  width: 100vw;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #f6f4f6;
}
.nose{
  width: 46vw;
  display: flex;
  justify-content: space-between;
}
.nose-item{
  width: 12vw;
  height: 12vw;
  background-color: #ccc;
}
.top image{
  width: 70vw;
  height: 7vw;
  margin-top: 100rpx;
}
.mid{
  height: 6rpx;
  width: 100vw;
  display: flex;
  justify-content: space-evenly;
}
.mid-item{
  width: 20vw;
  height: 100%;
}
.btm{
  flex: 1.3;
  width: 100vw;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.btm image{
  width: 70vw;
  height: 19.5vw;
}
.back{
  position: absolute;
  top: 5vh;
  left: 20rpx;
  width: 70rpx;
  height: 70rpx;
  z-index: 99;
}
.back image{
  width: 70rpx;
  height: 70rpx;
}