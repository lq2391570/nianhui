/* pages/applyFinishPage/applyFinishPage.wxss */
page{
  height: 100%;
}
.bgView{
  position: fixed;
  width: 100%;
  display: flex;
  flex-direction: column;
  background: white;
  height: 100%;
  justify-content: space-between;
  align-items: center;
  background-size: 100% 100%;
}
.contentView{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 90%;
  width: 100%;
}
.topContentView{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 95%;
  width: 100%;
  background-size: 100% 100%;
}
.topContentView-Top{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  height: 60vw;
}
.topContentView-Top image{
  height: 210rpx;
  width: 220rpx;
  margin-top: 60rpx;
}
.topContentView-Top text:nth-child(2){
  font-weight: bold;
  font-size: 30rpx;
  color: rgb(225, 78, 65);
  padding-bottom: 30rpx;
}
.topContentView-Top text:nth-child(3){
  font-weight: bold;
  font-size: 40rpx;
  color: rgb(225, 78, 65);
  padding-bottom: 30rpx;
}
.topContentView-Bottom{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  height: 60%;
}
.personalView{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-size: 100% 100%;
  width: 76%;
  min-height: 220rpx;
  max-height: 320rpx;
}
.personalView text:nth-child(1){
  font-size: 28rpx;
  color: gray;
  padding-top: 20rpx;
  
}
.personalView text:nth-child(2){
  font-size: 30rpx;
  padding: 20rpx;
  color: #666;
}
.noGroupPersonalView{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  text-align: center;
  font-size: 50rpx;
  height: 140rpx;
  color: rgb(225, 78, 65);
  font-weight: bold;
}
.hintTextView{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  font-size: 26rpx;
  color: rgb(103, 103, 103);
  text-align: center;
  height: 120rpx;
}
.bottomhintView{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin: 0rpx 30rpx;
  background: rgb(252, 246, 242);
  border-radius: 25rpx;
  height: 50rpx;
  width: 70%;
  margin-bottom: 40rpx;
}
.bottomhintView text:nth-child(1){
  font-size: 25rpx;
  color: rgb(200, 174, 157);
  line-height: 50rpx;
  text-align: center;
  margin-right: 25rpx;
}
.bottomhintView text:nth-child(2){
  font-size: 25rpx;
  color: rgb(200, 174, 157);
  line-height: 20rpx;
  text-align: center;
  border-bottom: 0.5rpx solid rgb(200, 174, 157);
  padding-top: 5rpx;
  margin-left: 25rpx;
}
.ensureSignView{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.ensureSignBtn{
  width: 584rpx;
  height: 124rpx;
  text-align: center;
  line-height: 115rpx;
  color: white;
}
.canNotSign{
  margin-top: 10rpx;
  color: #D2ABA7;
}
.canNotSign text{
border-bottom: 1rpx solid #D2ABA7;
}
.ensureCanNotSignView{
  position: absolute;
  width: 80%;
  height: 750rpx;
  margin: auto;
  top: 300rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.ensureCanNotSignView-top{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  height: 650rpx;
  width: 100%;
  background: white;
  border-radius:20rpx;
}
.btn-group {
    position: relative;
    z-index: 1010;
    margin-bottom: 50px;
}
.ensureCanNotSignView-top-subtitle{
  color: #666666;
  font-size: 30rpx;
  margin-top: 100rpx;
}
.ensureCanNotSignView-top-subtitle text{
  border-bottom: 1rpx solid #cccccc;
  padding-bottom: 20rpx;
}
.ensureCanNotSignView-top-title{
  color: rgb(225, 78, 65);
  font-size: 60rpx;
  font-weight: bold;
  margin-bottom: 50rpx;
}
.ensureCanNotSignView-top-btn{
  width: 584rpx;
  height: 124rpx;
  line-height: 114rpx;
  color: white;
  text-align: center;
  margin-bottom: 20rpx;
}

.ensureCanNotSignView-closeBtn{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100rpx;
  height: 100rpx;
  margin-top: 20rpx;
}
.ensureCanNotSignView-closeBtn image{
  width: 64rpx;
  height: 64rpx;
  
}
