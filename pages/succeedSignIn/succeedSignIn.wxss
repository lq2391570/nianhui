/* pages/succeedSignIn/succeedSignIn.wxss */
page{
  height: 100%;
}
.bgView{
  position: fixed;
  width: 100%;
  display: flex;
  flex-direction: column;
  background: white;
  height: 100%;
  justify-content: space-between;
  align-items: center;
  background-size: 100% 100%;
}
.contentView{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  height: 90%;
  width: 100%;
  background-size: 100% 100%;
  align-items: center;
}
.signInScceedTitle{
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 75%;
  height: 130rpx;
  align-items: center;
  background-size: 100% 100%;
  margin-top: 30rpx;
}
.signInPeoplesView{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  /* height: 100rpx; */
  width: 75%;
  padding-top: 20rpx;
}
.signInPeoplesView text:nth-child(1){
  padding-top: 20rpx;
  font-size: 28rpx;
  color: #666666;
}
.signInPeoplesView text:nth-child(2){
  font-size: 28rpx;
  color: #333333;
  padding-top: 20rpx;
}
.seatsView{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  height: 50%;
  width: 80%;
  flex-wrap: wrap;
  padding-bottom: 50rpx;
  margin-top: 30rpx;
  overflow-y: scroll;
  /* background: blue; */
}
.seats{
  padding: 5rpx;
  margin: 10rpx;
  height: 43rpx;
  width: 43rpx;
  font-size: 25rpx;
  text-align: center;
  line-height: 43rpx;
}

.signInScceedTitleText1{
  font-size: 30rpx;
  color: #777777;
}
.signInScceedTitleText2{
  font-size: 55rpx;
  color: rgb(225, 78, 65);
  font-weight: bold;
}



